{
    "name": "zarate-systems/laravel-cognito-auth",
    "description": "An authentication driver for Laravel for authenticating users in AWS Cognito User Pools.",
    "keywords": [
        "php",
        "laravel",
        "auth",
        "aws",
        "cognito",
        "zarate-systems",
        "laravel-cognito-auth"
    ],
    "homepage": "https://github.com/zarate-systems/laravel-cognito-auth",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Arran Jacques",
            "email": "arranjacques@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Erick Zarate",
            "email": "zaraterick@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "aws/aws-sdk-php-laravel": "^3.6",
        "illuminate/auth": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "ZarateSystems\\LaravelCognitoAuth\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZarateSystems\\LaravelCognitoAuth\\LaravelCognitoAuthServiceProvider"
            ]
        }
    }
}
